{
  "$schema": "https://www.krakend.io/schema/v3.json",
  "version": 3,
  "name": "KrakenD API Gateway (qa)",
  "port": 8080,
  "timeout": "300s",
  "cache_ttl": "3s",
  "output_encoding": "no-op",
  "extra_config": {
    "router": {
      "return_error_msg": true
