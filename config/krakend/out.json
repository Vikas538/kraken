      "endpoint": "/user/getUserDeatils",
      "method": "GET",
      "input_headers": [
        "*"
      ],
      "input_query_strings": [
        "*"
      ],
      "backend": [
        {
          "url_pattern": "/user/getUserDeatils",
          "method": "GET",
          "host": [
            "https://api-qa.beta.pharmconnect.com"
          ],
          "extra_config": {
            "plugin/http-client": {
              "name": "auth-plugin",
              "auth-plugin": {
                "host": "https://api-qa.beta.pharmconnect.com",
                "serverSecretName": "pharmconnect-qa-server-jwt-config",
                "jwtSecretName": "pharmconnect-qa-jwtConstants",
                "permissions": "collection_report_write,role_assignment_write"
              }
            }
