{
  "$schema": "https://www.krakend.io/schema/v3.json",
  "version": 3,
  "name": "KrakenD API Gateway (dev)",
  "port": 8080,
  "timeout": "300s",
  "cache_ttl":  "3s",
  "output_encoding": "json",
  "extra_config": {"router":{"return_error_msg":true}},
  "plugin":  {
    "pattern": ".so",
    "folder": "/opt/krakend/plugins/"
  },
    "output_encoding": "no-op",
  "endpoints": [
       
    {
        "endpoint": "/user/getUserDeatils",
        "method": "GET",
        "input_headers":["*"],
        "input_query_strings": ["*"],
        "backend": [
            {
              "url_pattern": "/user/getUserDeatils",
              "method": "GET",
              "host": ["http://member.dev-host.com"],
              "extra_config": {
                  "plugin/http-client": {
  "name": "krakend-client-example",
  "krakend-client-example": {
    "path": "https://api-dev.beta.pharmconnect.com"
  }
}
              }
            }
        ]
    },
    {
        "endpoint": "/test-2",
        "method": "GET",
        "input_headers":["*"],
        "input_query_strings": ["*"],
        "backend": [
            {
              "url_pattern": "/__debug/dev",
              "method": "GET",
              "host": ["http://member.dev-host.com"],
              "extra_config": {
                  "plugin/http-client": {
  "name": "krakend-client-example",
  "krakend-client-example": {
    "path": "https://api-dev.beta.pharmconnect.com"
  }
}
              }
            }
        ]
    },
    {
        "endpoint": "/user/getUserDeatils",
        "method": "POST",
        "input_headers":["*"],
        "input_query_strings": ["*"],
        "backend": [
            {
              "url_pattern": "/user/getUserDeatils",
              "method": "POST",
              "host": ["http://member.dev-host.com"],
              "extra_config": {
                  "plugin/http-client": {
  "name": "krakend-client-example",
  "krakend-client-example": {
    "path": "https://api-dev.beta.pharmconnect.com"
  }
}
              }
            }
        ]
    },
    {
        "endpoint": "/claims/return-orders/list/{workspaceId}",
        "method": "POST",
        "input_headers":["*"],
        "input_query_strings": ["*"],
        "backend": [
            {
              "url_pattern": "/claims/return-orders/list/{workspaceId}",
              "method": "POST",
              "host": ["http://member.dev-host.com"],
              "extra_config": {
                  "plugin/http-client": {
  "name": "krakend-client-example",
  "krakend-client-example": {
    "path": "https://api-dev.beta.pharmconnect.com"
  }
}
              }
            }
        ]
    }
 ]
}
