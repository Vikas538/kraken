            "endpoint": "/user/getUserDeatils",
            "method": "GET",
            "input_headers": [
                "*"
            ],
            "input_query_strings": [
                "*"
            ],
            "backend": [
                {
                    "url_pattern": "/user/getUserDeatils",
                    "method": "GET",
                    "host": [
